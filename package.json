{
  "name": "replica-island-reborn",
  "version": "1.0.0",
  "description": "Web port of Replica Island - a classic Android platformer game",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bun run typecheck && bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "replica-island",
    "game",
    "platformer",
    "react",
    "canvas",
    "typescript"
  ],
  "author": "",
  "license": "Apache-2.0"
}
